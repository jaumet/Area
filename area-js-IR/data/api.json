"nid":"184","name":"SETI@home","description":"SETI@home (\"SETI at home\") is an Internet-based public volunteer computing project employing the BOINC software platform, hosted by the Space Sciences Laboratory, at the University of California, Berkeley, in the United States. SETI is an acronym for the Search for Extra-Terrestrial Intelligence. Its purpose is to analyze radio signals, searching for signs of extra terrestrial intelligence, and is one of many activities undertaken as part of SETI. Everyone can participate by running a free program that downloads and analyzes radio telescope data obtained.","homepage":{"value":"http://setiathome.berkeley.edu","title":"NaN","attributes":"NaN","path":"http://setiathome.berkeley.edu","options":{"query":"NaN","fragment":"NaN","attributes":"NaN"}},"social_networks_accounts":[{"value":"https://twitter.com/setiathome","title":"Twitter","attributes":"NaN","path":"https://twitter.com/setiathome","options":{"query":"NaN","fragment":"NaN","attributes":"NaN"}},{"value":"https://www.facebook.com/pages/SETIhome/16063264533","title":"Facebook","attributes":"NaN","path":"https://www.facebook.com/pages/SETIhome/16063264533","options":{"query":"NaN","fragment":"NaN","attributes":"NaN"}}],"email":"sitihome@home.com","contact_page":{"value":"http://seti.berkeley.edu/seti_drupal/?q=contact","title":"NaN","attributes":"NaN","path":"contact","options":{"query":"NaN","fragment":"NaN","attributes":"NaN"}},"organisation_type":"Academia And Research","physical_address":{"country":"GB","administrative_area":"NaN","sub_administrative_area":null,"locality":"NaN","dependent_locality":null,"postal_code":"NaN","thoroughfare":"NaN","premise":"NaN","sub_premise":null,"organisation_name":null,"name_line":null,"first_name":null,"last_name":null,"data":null},"legal_entity":"public institution","year_of_foundation":"1999-01-01 00:00:00","languages":["English"],"phone":"NaN","keywords":"NaN","type_of_activity":["Collaborative research","Citizen science"],"mos